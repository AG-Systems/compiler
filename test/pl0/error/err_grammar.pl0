/* 这个例子演示了语法错，由于语法错的模式相似，所以只举例一部分 */
procedure a;
var v;
begin
	v = 2                        /* 13 */
	if (v=1) than v=3;           /* 10 16 */
	while v do begin end;        /* 20 */
end;

procedure b;
var do;                              /* 4 */
begin
     call a;
end;

begin
	call a;
	call b;
end;                                 /* 8 */
